[MappingDeclaration] @collection [[
mappingId	mapping-409400504
target		<http://id.vlaanderen.be/id/adres/{AdresID}> a <http://www.w3.org/ns/locn#Address> ; <http://purl.org/dc/terms/identifier> {AdresID} ; <http://www.w3.org/ns/locn#thoroughfare> {straatnaam_nl}@nl ; <http://www.w3.org/ns/locn#locatorDesignator> {Huisnummer} ; <http://www.w3.org/ns/locn#postName> {gemeentenaam_nl}@nl ; <http://www.opengis.net/ont/geosparql#sfTouches> <http://id.vlaanderen.be/id/straat/{StraatID}> ; <http://www.w3.org/ns/locn#postCode> {Postcode} . 
source		
			  SELECT a.AdresID, a.StraatID as straatid, a.Huisnummer, a.Busnummer, a.Postcode,
			sn.Naam as straatnaam_nl, gn.Naam as gemeentenaam_nl
			 FROM Adres a
			 INNER JOIN Straat s on a.StraatID=s.StraatID
			 INNER JOIN Straatnaam sn on s.StraatID=sn.StraatID
			 INNER JOIN Gemeente g on s.GemeenteID=g.GemeenteID
			 INNER JOIN Gemeentenaam gn on g.GemeenteID=gn.GemeenteID
			WHERE sn.TaalISOCode = 'NLD'
			AND gn.TaalISOCode = 'NLD'
			  

