<AdresMapping>
  a rr:TriplesMap;
  rr:logicalTable [ 
  rr:tableName "Adres" ;
  a rr:BaseTableOrView
  ];
  rr:subjectMap [
    rr:template "http://id.vlaanderen.be/id/adres/{AdresID}#id";
    rr:class locn:Address
  ]
;
  rr:predicateObjectMap [
    rr:predicate dcterms:identifier;
    rr:objectMap [
    rr:column "AdresID"
    ]
   ]
  .


   



